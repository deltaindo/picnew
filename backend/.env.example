NODE_ENV=development
PORT=5000

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/pic_app

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production-min-32-characters
JWT_REFRESH_SECRET=your-refresh-secret-key-min-32-characters
JWT_EXPIRY=24h

# AWS S3
AWS_REGION=ap-southeast-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
S3_BUCKET_NAME=pic-app-documents
S3_ENDPOINT=https://s3.amazonaws.com

# Email (SendGrid)
SENDGRID_API_KEY=your-sendgrid-key
SMTP_FROM=noreply@delta-indonesia.com

# WhatsApp (Twilio)
TWILIO_ACCOUNT_SID=your-twilio-sid
TWILIO_AUTH_TOKEN=your-twilio-token
TWILIO_PHONE_NUMBER=+1234567890

# Admin
ADMIN_EMAIL=admin@delta-indonesia.com
